#!/bin/sh
# $Revision: 2.3.2.2 $
# File to make links from this directory to the SOURCES directory for all files

HERE=`pwd`
BASE=`rpm --eval '%{_sourcedir}'`

echo Linking files to directory $BASE
for i in `cat postfix-filelist`; do
  test -f $i && test ! -x $i && \
    ln -s $HERE/$i $BASE/$i && echo "  $i"
done
